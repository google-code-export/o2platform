<H2>
  <SourceCode>panel.clear();
var controls = panel.add_1x1("steps", "data", false, 400);

var graphSteps = controls[0].add_Graph();
var graphData = controls[1].add_Graph();

graphSteps.layout("Tree");
graphData.layout("ISOM");

graphData.wpfInvoke(()=&gt; {
							var op = new GraphSharp.Algorithms.OverlapRemoval.OverlapRemovalParameters();
							op.HorizontalGap = 250;  
							op.VerticalGap = 250;  
							graphData.OverlapRemovalParameters = op;
							});

graphSteps.edge("Workflow", "loading xml");
var xml = "C:\\O2\\FileSamples\\XmlFiles\\OwaspWikiResponse.xml".fileContents().xmlFormat();
graphSteps.edgeFromLast("from xml creating  xsd");
var xsd = xml.xmlCreateXSD();
graphSteps.edgeFromLast("from xsd creating  CSharpFile");
var csharp = xsd.xsdCreateCSharpFile();
graphSteps.edgeFromLast("compiling source code");
var assembly = csharp.compile();
graphSteps.edgeFromLast("from assembly loading object");
var data = assembly.xsdAssemblyGetObject(xml);
graphSteps.edgeFromLast("showing data");
//return null;
//graph.add(xml);

var xmlNode = graphData.add_WpfTextEditor().set_Text(xml).xml().width(400).height(400);
var xsdNode = graphData.add_WpfTextEditor().set_Text(xsd).xml().width(400).height(400);
var csharpNode = graphData.add_WpfTextEditor().set_Text(csharp.fileContents()).csharp().width(400).height(400);
var assemblyNode = graphData.add_WpfTextEditor().set_Text(assembly.str()).width(300);
var dataNode = graphData.add_WpfTextEditor().set_Text(data.str()).xml().width(400).height(400);

graphData.edge(xmlNode, xsdNode); 
graphData.edge(xsdNode, csharpNode);
graphData.edge(csharpNode, assemblyNode);
graphData.edge(assemblyNode, dataNode);
