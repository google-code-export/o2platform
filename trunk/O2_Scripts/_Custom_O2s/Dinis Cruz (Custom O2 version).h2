topPanel.insert_Below&lt;Panel&gt;(110).add_LogViewer(); 
var leftPanel = topPanel.insert_Right&lt;Panel&gt;(200).fill(false).width(200).height(148);

topPanel.parentForm().top(60).left(60); 

leftPanel.add_PictureBox().show("O2_Version_For_DinisCruz.jpg".local()).fill(false).width(190).height(140);;
topPanel.backColor("Control"); 
topPanel.parent&lt;SplitContainer&gt;().backColor(Color.White);//.splitterDistance(200); 
 
var logoNoShadow = "O2_Logo_no_Shadow.jpg".local();  
var smallLogo = "O2Logo_Small.gif".local();

var ribbon = topPanel.add_Control&lt;WPF_Ribbon&gt;(); 

//Custom O2 tab
ribbon.add_Tab("Custom O2s") 	

	  .add_Group("Main GUIs")
