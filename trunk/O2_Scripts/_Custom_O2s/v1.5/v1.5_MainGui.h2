browser.beforeNavigate(
	(url)=&gt; {	
				var fileName = url.fileName();
				if (fileName.extension(".h2"))
				{
					O2Thread.mtaThread(
						()=&gt;{
								if (url.fileExists()) 
									url.executeH2Script();
								else
									fileName.local().executeH2Script();
							});
					
					return true;
				}
				return false;
			});
		
