var allTmUnitTestsWrapperFile = "All TM Unit Tests.cs".local();
 
var loadFile = (Func&lt;string,Panel&gt;)"Execute Unit Tests.h2".executeFirstMethod();
var topPanel = loadFile(allTmUnitTestsWrapperFile);

topPanel.parentForm().set_Text("TeamMentor - AllUnit Tests").width(700).height(400);

//topPanel.closeForm_InNSeconds(5);
topPanel.control&lt;TreeView&gt;()
	   .expandAll()
	   .showSelection()
	   .selectFirst();

var textBox =  topPanel.parentForm()
					   .control&lt;TextBox&gt;(true);
textBox.add_ContextMenu()
	   .add_MenuItem("Edit UnitTests", true,
	   					()=&gt; PublicDI.CurrentScript.parentFolder()
	   											   .showCodeEditorForFilesInFolder("*.cs"))
		.add_MenuItem("Open In Unit Tests Editor", 
	   					()=&gt;{
	   							var unitTestsEditor_loadFile = (Action&lt;string&gt;)"Write and Execute Unit Tests.h2".executeFirstMethod();
	   							unitTestsEditor_loadFile(textBox.get_Text());
	   						});

return "done";
